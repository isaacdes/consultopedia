<html>
    <head>
        <title>Admin Dashboard</title>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        <link rel="stylesheet" href="css/style.css">
        <%- include('head2'); %>
    </head>
    <body>
        <!-- include nav bar -->
        

        <!-- admin section -->
        <div class="container-fluid Main_Body">
          <div class="container-fluid">
            <div class="row">
              <!-- side bar -->
              <div class="col-1 Slide_Bar">

                <ul class="nav flex-column nav-pills nav-fill">
                  <li class="nav-item Nav_Pad">
                    <a class="nav-link active" aria-current="page" href="Admin_Dashboard">Dashboard</a>
                  </li>
                  <li class="nav-item Nav_Pad">
                    <a class="nav-link " href="Admin_Session">Session </a>
                  </li>
                  <li class="nav-item Nav_Pad">
                    <a class="nav-link" href="Admin_Customers">Customers</a>
                  </li>
                  <li class="nav-item Nav_Pad">
                    <a class="nav-link" href="Admin_Counselors">Counselors</a>
                  </li>
                  
                </ul>
              </div>

              <div class="col-1 "></div>
              <!-- body -->
              

              <div class="col-10 Main_Data">
                
                
                <div class="row">

                  <div class="col-4">
                    <div class="card ad-card" style="width: 18rem;">
                      <img class="card-img-top" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcS4CYlKu3iOg5g8G_ySC-HeFbeyBIxBLEMWkg&usqp=CAU" alt="Card image cap">
                      <div class="card-body"><center>
                        <h5 class="card-title">Counselors</h5>
                        <%i=0%>
                        <%user.forEach(item=>{%>
                        <%if(item.role==='counselor'||item.role==='Counselor'){i=i+1}})%>
                        <p class="card-text">No of counselors:<h1><%=i%></h1></center></p>
                        <a href="Admin_Counselor.html" class="btn btn-primary">Counselors</a>
                      </div>
                    </div>
                  </div>
                  <div class="col-4">
                    <div class="card ad-card" style="width: 18rem;">
                      <img class="card-img-top" src="https://cdn.searchenginejournal.com/wp-content/uploads/2021/06/5-ways-to-find-new-foreign-customers-for-your-website-60db14c6602cd-1520x800.png" alt="Card image cap">
                      <div class="card-body">
                        <h5 class="card-title">Customers</h5>
                        <%i=0%>
                        <%user.forEach(item=>{%>
                        <%if(item.role==='user'||item.role==='User'){i=i+1}})%>
                        <p class="card-text">Number of Customers:<center><h1><%=i%></h1></center></p>
                        <a href="Admin_Customers.html" class="btn btn-primary">Customers</a>
                      </div>
                    </div>
                  </div>

                  

                  <div class="col-4">
                    <div class="card ad-card" style="width: 18rem;">
                      <img class="card-img-top" src="https://www.mindshealer.com/wp-content/uploads/elementor/thumbs/sakshi-blog-osigvrjhq3b3bpxml5znvcnrxi9ii36gpnvmc0w2h4.png" alt="Card image cap">
                      <div class="card-body">
                        <h5 class="card-title">Sessions</h5>
                        <p class="card-text">Total no of sessions:<center><h1><%=bookings.length%></h1></center></p>
                        <a href="Admin_Session.html" class="btn btn-primary">Sessions</a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              
            </div>
          </div>
        </div>
        

        

        
        
        
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    </body>
</html>