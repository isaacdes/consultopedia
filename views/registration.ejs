<html>

<head>
    <title>Admin Dashboard</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous" />
    
    <script type="text/javascript" src="Validate.js"></script>
    <link rel="stylesheet" href="css/style.css" />
        <%- include('partials/head'); %>
</head>

<body>
    <!-- include nav bar -->
    





    <!-- admin section -->
    <div class="container-fluid Main_Body">
        
      <div class="row">
          <!-- side bar -->

          <!-- body -->

          <div class="col-12 ">
              <img src="https://www.rxdx.in/wp-content/uploads/2020/04/Consult-doctor-online-Telemedicine.jpg"
                  class="img-fluid" alt="Responsive image">

          </div>

          <div class="container Main_Data">
              <div class="row ">
                  
              
                <div class="col-1"></div> 
                <div class="col-11"><h1>Registration Form</h1></div>
                 <div class="col-1"></div> 
                <div class="col-10">
                  <form
                    action="/registration"
                    id="form"
                    name="myform"
                    method="post"
                    onsubmit="return Validateform()"
                  >
                    <div class="form-group">
                      <label for="forName">Full Name</label>
                      <input
                        
                        type="text"
                        class="form-control"
                        id="name"
                        name="name"
                        placeholder="Name"
                        
                            
                      />
                      
                      <label for="forName">Email ID</label>
                      <input
                        type="email"
                        class="form-control"
                        id="email"
                        name="email"
                        placeholder="Email"
                            
                      />
                      <label for="forName">Organization</label>
                      <input
                        type="text"
                        class="form-control"
                        id="org"
                        name="organization"
                        placeholder="Organization"
                            
                      />
                      <label for="forName">Address</label>
                      <input
                        type="text"
                        class="form-control"
                        id="add"
                        name="address"
                        placeholder="Address"
                            
                      />
                      <div class="row custom-row">
                        <div class="col-sm-4">
                          <input
                            type="text"
                            class="form-control raw"
                            id="country"
                            name="country"
                            placeholder="Country"
                                
                          />
                        </div><br>
                        <div class="col-sm-4">
                          <input
                            type="number"
                            class="form-control raw"
                            name="pincode"
                            id="pin"
                            placeholder="Pincode"
                                
                          />
                        </div>
                      </div><br>
                      <label for="forName">Contact</label>
                      <input
                        type="text"
                        class="form-control"
                        id="contact"
                        name="phone"
                        placeholder="Phone Number"
                            
                      /><br>
                      <label for="forName">Aadhar Card Details</label>
                      <input
                        type="text"
                        class="form-control"
                        id="aadhar"
                        name="aadharNum"
                        placeholder="Aadhar number"
                            
                      /><br>
                      <label for="forName">Password</label>
                      <input
                        type="text"
                        class="form-control"
                        name="password"
                        id="password"
                        placeholder="min 6 characters"
                            
                      /><br>
                      <input
                        type="text"
                        class="form-control"
                        id="formEmail"
                        placeholder="Repeat Password"
                            
                      /><br>
                      <label for="forName">Role</label>
                      
                      
                      <select name="role" id="role">
                        <optgroup label="Role">
                          <option value="user">Customer</option>
                          <option value="counselor">Counselor</option>
                      </optgroup>
                      </select>

                    </div>
        
                    <input
                      class="btn btn-primary btn-lg btnCenter"
                      type="submit"
                      value="Register ME !"
                          
                    />
                    </div>

        
                    
                  </form>
                </div>
                <div class="col"></div>
              </div>
                  
                      
                        
                          
                  
                      
                      
              
                        
                      
                    
                  

              </div>
          </div>
      </div>

  
      </div>
      <%- include('partials/Footer'); %>
  </div>
</div>




    
    

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>
</body>

</html>