<html>
    <head>
        <title>Admin Dashboard</title>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        <link rel="stylesheet" href="css/style.css">
    </head>
    <body>
        <!-- include nav bar -->
        
        <!-- admin section -->
        <div class="container-fluid Main_Body">
          <div class="container-fluid">
            <div class="row">
              <!-- side bar -->
              <div class="col-1 Slide_Bar">

                <ul class="nav flex-column nav-pills nav-fill">
                  <li class="nav-item Nav_Pad">
                    <a class="nav-link" href="Admin_Dashboard">Dashboard</a>
                  </li>
                  <li class="nav-item Nav_Pad">
                    <a class="nav-link active" aria-current="page" href="Admin_Session">Session History</a>
                  </li>
                  <li class="nav-item Nav_Pad">
                    <a class="nav-link" href="Admin_Customers">Customers</a>
                  </li>
                  <li class="nav-item Nav_Pad">
                    <a class="nav-link" href="Admin_Counselor">Counselors</a>
                  </li>
                  
                </ul>
              </div>

              <div class="col-1 "></div>
              <!-- body -->
              <div class="col-10 col-sm Main_Data">
                <table class="table">
                  <thead>
                    <tr>
                      <th scope="col">#</th>
                      <th scope="col">UserID</th>
                      <th scope="col">CounselorId</th>
                      
                      <th scope="col">date</th>
                      <th scope="col">time</th>
                      <th scope="col">status</th>
                    </tr>
                  </thead>
                  <tbody>
                    <%let i=0%>
                     <%booking.forEach(item=>{%>
                            <tr ><form action="/Admin_Customers" method="post">
                              <%i=i+1%>
                              <th scope="row"><input name="id" value="<%=item._id%>" type="hidden" readonly><%=i%></th>
                              <td><input name="name" value="<%= item['user_id'] %>"  readonly></td>
                              <td><input name="email" value="<%=item['counselor_id']%>"  readonly></td>
                              <!-- <td><input name="address" value="<%=item['counselor_name']%>"  readonly></td> -->
                              <td><input name="country" value="<%=item['date']%>"  readonly></td>
                              <td><input name="aadhar" value="<%=item['time']%>" readonly></td>
                              <td><input name="aadhar" value="<%=item['status']%>" readonly></td>
                              <!--<td><input class="btn btn-primary btn-sm btnCenter"  type="submit" value="update" required/></td>
                              
                            </form>
                            <form action="/delete" method="post">
                            <input name="id" value="<%=item._id%>" type="hidden" readonly>
                            <td><input class="btn btn-primary btn-sm btnCenter"  type="submit" value="delete" required/></td>
                            </form> -->
                            </tr>
                     <%})%> 
                    
                  </tbody>
                </table>
              </div>
              
            </div>
          </div>
        </div>
        

        

        
        
        
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    </body>
</html>